{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-AB-01"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "cp_monster_to_ab",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"sink": {
								"type": "AzureSqlSink",
								"tableOption": "autoCreate",
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "monster_vipdatabase",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "ab_monsterdb",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/monster_vipdatabase')]",
				"[concat(variables('factoryId'), '/datasets/ab_monsterdb')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ab_monsterdb')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ab_monstersqlservera_MonsterData_ls",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "ab_datEng",
					"table": "vip_depletion_fact_load"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/monster_vipdatabase')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "vipdataset_ls",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "Servicing Distributor ID",
						"type": "varchar"
					},
					{
						"name": "Callpoint ID",
						"type": "varchar"
					},
					{
						"name": "Invoice Date",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "Item Code",
						"type": "varchar"
					},
					{
						"name": "Cases",
						"type": "decimal",
						"precision": 38,
						"scale": 12
					},
					{
						"name": "Did Buys",
						"type": "int",
						"precision": 10
					},
					{
						"name": "dateRange",
						"type": "varchar"
					},
					{
						"name": "Run_ID",
						"type": "nvarchar"
					},
					{
						"name": "DtCreated",
						"type": "nvarchar"
					},
					{
						"name": "CreatedBy",
						"type": "nvarchar"
					}
				],
				"typeProperties": {
					"schema": "ab_datEng",
					"table": "[[ab_datEng].[vip_depletion_fact_load]"
				}
			},
			"dependsOn": []
		}
	]
}
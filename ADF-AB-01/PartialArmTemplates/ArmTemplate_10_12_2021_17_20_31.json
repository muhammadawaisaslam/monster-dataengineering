{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-AB-01"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/monster_vipdatabase')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "vipdataset_ls",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "Callpoint ID",
						"type": "nvarchar"
					},
					{
						"name": "Item Code",
						"type": "nvarchar"
					},
					{
						"name": "Authorization",
						"type": "nvarchar"
					},
					{
						"name": "Order",
						"type": "bigint",
						"precision": 19
					}
				],
				"typeProperties": {
					"schema": "ab_datEng",
					"table": "Bottlers"
				}
			},
			"dependsOn": []
		}
	]
}
{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-AB-01"
		},
		"ab_monstersqlservera_MonsterData_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ab_monstersqlservera_MonsterData_ls'"
		},
		"vipdataset_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'vipdataset_ls'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-AB-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ab_monstersqlservera_MonsterData_ls_connectionString": {
						"value": "[parameters('ab_monstersqlservera_MonsterData_ls_connectionString')]"
					},
					"vipdataset_ls_connectionString": {
						"value": "[parameters('vipdataset_ls_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-AB-01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-AB-01_ArmTemplate_0')]"
			]
		}
	]
}